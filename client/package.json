{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts-ts": "2.8.0",
    "styled-components": "^2.2.4"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && rm -rf ../src/main/resources/static && cp -r build ../src/main/resources/static",
    "test": "react-scripts-ts test --env=jsdom",
    "contracts": "react-scripts-ts test --env=jsdom --testMatch='<rootDir>/src/**/?(*.)contract.ts?(x)'",
    "eject": "react-scripts-ts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "generate-repo": "./scripts/generate_repository.sh $1 $2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.4",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^21.1.5",
    "@types/node": "^8.0.50",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "proxy": "http://localhost:8080"
}
